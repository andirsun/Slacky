{
  "name": "slack-arm64",
  "version": "0.0.1",
  "description": "Slack on electron for Linux arm64",
  "main": "dist/app/app.js",
  "scripts": {
    "start": "electron .",
    "build": "swc build ./src -d dist/app",
    "watch": "nodemon --watch src --ext ts --exec \"npm run build\"",
    "dev": "concurrently \"npm:watch\" \"npm:start\"",
    "pack": "npm run build && USE_SYSTEM_FPM=1 electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andirsun/slack-arm64.git"
  },
  "author": "Anderson Laverde <hola@andersonlaverde.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andirsun/slack-arm64/issues",
    "email": "hola@andersonlaverde.com"
  },
  "homepage": "https://github.com/andirsun/slack-arm64#readme",
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.0.2"
  }
}
